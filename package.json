{
  "name": "romanian-personal-identity-code-validator",
  "version": "1.0.0",
  "title": "Romanian personal identity code validator - CNP",
  "description": "Validates romanian personal identity codes (CNP). Validator cod numeric personal (CNP)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alceanicu/romanian-personal-identity-code-validator.git"
  },
  "keywords": [
    "CNP",
    "Validator"
  ],
  "author": "Nicu Alcea <nicu.alcea@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "moment": "^2.25.1"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "codecov": "^3.6.5",
    "jest": "^25.5.4",
    "prettier": "^2.0.5",
    "ts-jest": "^25.4.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "files": [
    "lib/**/*"
  ]
}
